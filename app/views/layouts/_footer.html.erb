<div class="all-page-modals">
                <div class="modal-container" data-modal-index="0">
                    <div class="modal-content">
                        <section class="unpad ">
                            <div class="container">
                                <div class="row">
                                    <div class="col-sm-6 col-md-offset-3 col-sm-offset-3 col-xs-12">
                                        <div class="boxed boxed--lg bg--white text-center feature">
                                            <div class="modal-close modal-close-cross"></div>
                                            <h3>Login to Your Account</h3>
                                            <a class="btn block btn--icon bg--facebook type--uppercase" href="http://trystack.mediumra.re/home-landing-2.html#">
                                                <span class="btn__text">
                                                    <i class="socicon-facebook"></i>
                                                    Login with Facebook
                                                </span>
                                            </a>
                                            <a class="btn block btn--icon bg--twitter type--uppercase" href="http://trystack.mediumra.re/home-landing-2.html#">
                                                <span class="btn__text">
                                                    <i class="socicon-twitter"></i>
                                                    Login with Twitter
                                                </span>
                                            </a>
                                            <hr data-title="OR">
                                            <div class="feature__body" id="login-dp" >
                                                <%= form_for Spree::User.new, :as => :spree_user, :url => spree.create_new_session_path do |f| %>
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <%= f.label :email, Spree.t(:email) %>
                                                            <%= f.email_field :email, :class => 'form-control', :tabindex => 1, autofocus: true %>
                                                        </div>
                                                        <div class="col-sm-12">
                                                            <%= f.label :password, Spree.t(:password) %>
                                                            <%= f.password_field :password, :class => 'form-control', :tabindex => 2 %>
                                                        </div>
                                                        <div class="col-sm-12">
                                                            <%= f.submit Spree.t(:login), :class => 'btn btn--primary type--uppercase', :tabindex => 4 %>
                                                        </div>
                                                    </div>
                                                    <!--end of row-->
                                                <% end %>

                                                <span class="type--fine-print block">Dont have an account yet?
                                                    <a href="javascript:;" id="signup">Create account</a>
                                                </span>
                                                <span class="type--fine-print block">Forgot your username or password?
                                                        <a href="http://trystack.mediumra.re/home-landing-2.html#">Recover account</a>
                                                </span>
                                            </div>
                                            <div class="feature__body hide" id="signup-dp" >
                                                <%= form_for Spree::User.new, :as => :spree_user, :url => spree.registration_path do |f| %>
                                                    <div data-hook="signup_inside_form" class="row">
                                                      <%= render :partial => 'spree/shared/user_form', :locals => { :f => f } %>
                                                        <div class="col-sm-12">
                                                            <%= f.submit Spree.t(:create), :class => 'btn btn--primary type--uppercase register-button' %>
                                                        </div>
                                                    </div>
                                                <% end %>

                                                <span class="type--fine-print block">already have an account?
                                                        <a href="#" id="login">login</a>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--end of row-->
                            </div>
                            <!--end of container-->
                        </section>
                    </div>
                </div>
                <div class="modal-container" data-modal-index="1">
                    <div class="modal-content">
                        <section class="imageblock feature-large bg--white border--round ">
                            <div class="imageblock__content col-md-5 col-sm-3 pos-left">
                                <div class="background-image-holder" style="background: url(&quot;<% asset_path("cowork-11.jpg") %>&quot;); opacity: 1;">
                                    <!-- <img alt="image" src="./Stack Multipurpose HTML Template_files/cowork-11.jpg"> -->
                                    <%= image_tag asset_path("cowork-11.jpg", class: "testimonial__image" ) %>
                                </div>
                            </div>
                            <div class="container">
                                <div class="row">
                                    <div class="col-md-5 col-md-push-6 col-sm-7 col-sm-push-4">
                                        <h2>Create an account</h2>
                                        <p class="lead">Get started with a 14 day free trial, No credit card required — cancel at any time.</p>
                                        <a class="btn block btn--icon bg--facebook type--uppercase" href="http://trystack.mediumra.re/home-landing-2.html#">
                                            <span class="btn__text">
                                                <i class="socicon-facebook"></i>
                                                Sign up with Facebook
                                            </span>
                                        </a>
                                        <a class="btn block btn--icon bg--twitter type--uppercase" href="http://trystack.mediumra.re/home-landing-2.html#">
                                            <span class="btn__text">
                                                <i class="socicon-twitter"></i>
                                                Sign up with Twitter
                                            </span>
                                        </a>
                                        <hr data-title="OR">
                                        <form>
                                            <div class="row">
                                                <div class="col-xs-12">
                                                    <input type="email" name="Email Address" placeholder="Email Address">
                                                </div>
                                                <div class="col-xs-12">
                                                    <input type="password" name="Password" placeholder="Password">
                                                </div>
                                                <div class="col-xs-12">
                                                    <button type="submit" class="btn btn--primary type--uppercase">Create Account</button>
                                                </div>
                                                <div class="col-xs-12">
                                                    <span class="type--fine-print">By signing up, you agree to the
                                                        <a href="http://trystack.mediumra.re/home-landing-2.html#">Terms of Service</a>
                                                    </span>
                                                </div>
                                            </div>
                                            <!--end row-->
                                        </form>
                                    </div>
                                </div>
                                <!--end of row-->
                            </div>
                            <!--end of container-->
                        </section>
                        <div class="modal-close modal-close-cross">
                            
                        </div>
                    </div>
                </div>
